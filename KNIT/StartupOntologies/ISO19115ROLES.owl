<?xml version="1.0" encoding="utf-8"?>
<rdf:RDF
  xmlns:ns1="http://ontology.test.es/comment#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/owner">
    <rdfs:subClassOf>
      <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing">
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">THING</rdfs:label>
      </rdf:Description>
    </rdfs:subClassOf>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">owner</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party that owns the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/mediator">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">mediator</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'a class of entity that mediates access to the resource and for whom the resource is intended or useful'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/pointOfContact">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">pointOfContact</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who can be contacted for acquiring knowledge about or acquisition of the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/custodian">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">custodian</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party that accepts accountability and responsibility for the resource and ensures appropriate care and maintenance of the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/stakeholder">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">stakeholder</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who has an interest in the resource or the use of the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/rightsHolder">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">rightsHolder</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party owning or managing rights over the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/coAuthor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">coAuthor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who jointly authors the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/contributor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">contributor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party contributing to the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/principalInvestigator">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">principalInvestigator</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'key party responsible for gathering information and conducting research'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/author">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">author</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who authored the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/publisher">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">publisher</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who published the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/user">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">user</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who uses the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/resourceProvider">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">resourceProvider</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party that supplies the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/distributor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">distributor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who distributes the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/collaborator">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">collaborator</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who assists with the generation of the resource other than the principal investigator'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/originator">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">originator</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who created the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/funder">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">funder</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party providing monetary support for the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/sponsor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">sponsor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'This definition sounds like an 'advocate'. Elsewhere the term'sponsor' is used more for customer or client (e.g. see definition of MD_ProgressCode/accepted)', 'party who speaks for the resource'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/processor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">processor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who has processed the data in a manner such that the resource has been modified'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <owl:AnnotationProperty rdf:about="http://ontology.test.es/comment#definition">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="http://ontology.test.es/comment#synonym">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
  </owl:AnnotationProperty>
  <owl:AnnotationProperty rdf:about="http://ontology.test.es/comment#ontology_acronm">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2000/01/rdf-schema#comment"/>
  </owl:AnnotationProperty>
  <owl:Class rdf:about="http://def.isotc211.org/iso19115/-1/2014/CitationAndResponsiblePartyInformation/code/CI_RoleCode/editor">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">editor</rdfs:label>
    <ns1:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'party who reviewed or modified the resource to improve the content'</ns1:definition>
    <ns1:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ISO19115ROLES</ns1:ontology_acronm>
  </owl:Class>
  <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Class">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">None</rdfs:label>
  </rdf:Description>
</rdf:RDF>
