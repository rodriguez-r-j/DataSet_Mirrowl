    <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'A physical entity is an entity that is spatio-temporally located (is or occupies some part of spacetime) and only has physical entities as parts.'</ns2:definition>
    <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GEXO</ns2:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C43789">
    <rdfs:subClassOf rdf:resource="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C43643"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gene_Product_Encoded_By_Gene</rdfs:label>
    <ns2:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gene_Product_Encoded_By_Gene</ns2:synonym>
    <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'A role used to assert a relationship between a gene product and the gene that encodes it. The domain and range for this role are 'Gene Product' and 'Gene', respectively.'</ns2:definition>
    <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCIT</ns2:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C616">
    <rdfs:subClassOf rdf:resource="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C718"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lipid</rdfs:label>
    <ns2:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lipid, Lipids</ns2:synonym>
    <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'A class of hydrocarbon-containing organic compounds. Lipids are insoluble in water but soluble in nonpolar solvents and play important roles in living organisms: these roles include functioning as energy storage molecules, serving as structural components of cell membranes, and constituting important signaling molecules. Lipids can be subdivided into 2 groups: fatty acids and glycerides.'</ns2:definition>
    <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCIT</ns2:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C1913">
    <rdfs:subClassOf rdf:resource="http://ncicb.nci.nih.gov/xml/owl/EVS/Thesaurus.owl#C1908"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drug or Chemical by Structure</rdfs:label>
    <ns2:synonym rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drug or Chemical by Structure</ns2:synonym>
    <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'Substances that are grouped or classified by chemical composition.'</ns2:definition>
    <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">NCIT</ns2:ontology_acronm>
  </owl:Class>
