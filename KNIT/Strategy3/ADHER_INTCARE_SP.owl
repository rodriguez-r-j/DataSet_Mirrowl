        <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'An abstract entity is an entity that exhibits zero, one or more physical manifestations.'</ns2:definition>
        <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GEXO</ns2:ontology_acronm>
      </rdf:Description>
