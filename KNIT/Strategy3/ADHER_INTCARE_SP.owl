    <ns2:definition rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'A gene is part of a nucleic acid that contains all the necessary elements to encode a functional transcript.'</ns2:definition>
    <ns2:ontology_acronm rdf:datatype="http://www.w3.org/2001/XMLSchema#string">GEXO</ns2:ontology_acronm>
  </owl:Class>
  <owl:Class rdf:about="http://semanticscience.org/resource/SIO_000003">
    <owl:disjointWith>
      <rdf:Description rdf:about="http://semanticscience.org/resource/SIO_000260">
